{
  "name": "cdk-datadog",
  "version": "0.0.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist pack doc 'lib/**/*.js' 'lib/**/*.d.ts' 'tst/**/*.js' 'tst/**/*.d.ts'",
    "test": "jest",
    "doc": "typedoc --excludeExternals --mode file --out doc lib/",
    "build": "jsii",
    "watch": "jsii -w",
    "integ": "cdk-integ",
    "package": "jsii-pacmak",
    "cdk": "cdk",
    "upload": "npm publish pack/js/* && python3 -m twine upload pack/python/*"
  },
  "author": {
    "name": "Philipp Garbe"
  },
  "awscdkio": {
    "twitter": "pgarbe"
  },  
  "repository": {
    "type": "git",
    "url": "https://github.com/pgarbe/cdk-datadog"
  },
  "license": "Apache-2.0",
  "jsii": {
    "outdir": "pack",
    "targets": {
      "python": {
        "distName": "cdk-datadog",
        "module": "cdk_datadog"
      }
    }
  },
  "keywords": [
    "cdk",
    "aws-cdk",
    "construct",
    "aws",
    "pgarbe",
    "datadog"
  ],  
  "devDependencies": {
    "@aws-cdk/assert": "1.28.0",
    "@aws-cdk/cloudformation-diff": "1.28.0",
    "@types/jest": "25.1.2",
    "@types/node": "13.9.1",
    "@types/nodeunit": "0.0.30",
    "jest": "25.1.0",
    "jsii": "1.13.0",
    "jsii-pacmak": "1.1.0",
    "nodeunit": "0.11.3",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "typedoc": "0.17.1",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "@aws-cdk/core": "^1.21.1",
    "@aws-cdk/aws-cloudformation": "^1.21.1",
    "@aws-cdk/aws-lambda": "^1.21.1",
    "@aws-cdk/aws-sns": "^1.21.1",
    "@aws-cdk/aws-sns-subscriptions": "^1.21.1",
    "@aws-cdk/aws-sqs": "^1.21.1",
    "@aws-cdk/aws-ecs": "^1.21.1",
    "@aws-cdk/aws-ssm": "^1.21.1",
    "@aws-cdk/custom-resources": "^1.21.1"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^1.21.1",
    "@aws-cdk/aws-ecs": "^1.21.1",
    "@aws-cdk/aws-lambda": "^1.21.1",
    "@aws-cdk/aws-sns": "^1.21.1",
    "@aws-cdk/aws-sns-subscriptions": "^1.21.1",
    "@aws-cdk/aws-sqs": "^1.21.1",
    "@aws-cdk/aws-ssm": "^1.21.1",
    "@aws-cdk/core": "^1.21.1",
    "@aws-cdk/custom-resources": "^1.21.1"
  }
}
